{txt}  5{com}.         local dep_vars `varlist' // creating a local for dep. variables
{txt}  6{com}.         local M = `:word count `dep_vars''
{txt}  7{com}. 
.         /* making longer local for regression because reg takes only one local */
.         local regressors1               `treat' `controls1'
{txt}  8{com}.         local regressors2               `treat' `controls2'
{txt}  9{com}.         
. 
.         // Initializing all the matices 
.         /*      we have to have separate matrices for each number format we want, I 
>                 think. We can't store control means and counts in the same matrix 
>                 because obviously we don't want counts to have decimal places, but 
>                 we do want decimal places for means. */
.         mat ate_g1                      = J(`M', 1, .)
{txt} 10{com}.         mat ate_g2                      = J(`M', 1, .)
{txt} 11{com}. 
.         mat pop_g1                      = J(`M', 1, .)
{txt} 12{com}.         mat pop_g2                      = J(`M', 1, .)
{txt} 13{com}. 
.         mat net_g1                      = J(`M', 1, .)
{txt} 14{com}.         mat net_g2                      = J(`M', 1, .)
{txt} 15{com}. 
.         mat net_all             = J(`M', 1, .)
{txt} 16{com}. 
. 
.         // Initializing row names.
.         mat rownames ate_g1     = `dep_vars'
{txt} 17{com}.         mat rownames ate_g2     = `dep_vars'
{txt} 18{com}. 
.         mat rownames net_g1     = `dep_vars'
{txt} 19{com}.         mat rownames net_g2     = `dep_vars'
{txt} 20{com}.         mat rownames net_all    = `dep_vars'
{txt} 21{com}. 
. 
. 
. 
.         /*      This upcoming for loop does the following: 
>                 1)      For each dependent variable it runs a regression of y on treat and 
>                         the covariates. 
>                 2)      Calculates the p-values and significance levels of the coefficients
>                 3)      Inputs the estimates for beta (on treat only), into the matrix. It 
>                         inputs beta hat in one column and SE hat in another. 
>                 
>                 In order to understand why I generate stars the way I do, please see 
>                 this link: https://www.pdx.edu/econ/sites/www.pdx.edu.econ/files/frmttable_sj.pdf
>                 frmttable adds stars to estimates based on a matrix that here i call 
>                 stars. If stars(i,j) = 3, for example, than cell (i,j) will get 3 stars. 
>                 I set the first column of stars all to zero because we want stars to 
>                 go next to the standard errors, not the beta estimates. 
>         */
.         loc m = 1
{txt} 22{com}.         foreach y in `dep_vars'{c -(} 
{txt} 23{com}.                 
.                 ************************************************************************
.                 * Year 1 regression  ***************************************************
.                 ************************************************************************
. 
.                 * Check to make sure we have observations of that variable 
.                 qui sum `y' if `group1' == 1
{txt} 24{com}.                 if r(N) == 0 {c -(}
{txt} 25{com}.                 display as error "Variable is all missing in Year 1"
{txt} 26{com}.                 exit 
{txt} 27{com}.                 {c )-}       
{txt} 28{com}.                 
.                 qui svy: reg `y' `regressors1' if (`group1'== 1 & !missing(`y'))
{txt} 29{com}.                 mat ate_g1[`m', 1] = _b[`treat']
{txt} 30{com}. 
. 
. 
.                 ************************************************************************
.                 * Year 2 regression ****************************************************
.                 ************************************************************************
.                 * Check to make sure we have observations of that variable 
.                 qui sum `y' if `group2' == 1
{txt} 31{com}.                 if r(N) == 0 {c -(}
{txt} 32{com}.                 display as error "Variable is all missing in Year 2"
{txt} 33{com}.                 exit 
{txt} 34{com}.                 {c )-}       
{txt} 35{com}.                 
.                 qui svy: reg `y' `regressors2' if (`group2'== 1 & !missing(`y'))
{txt} 36{com}.                 mat ate_g2[`m', 1] = _b[`treat']
{txt} 37{com}.                 local ++m       
{txt} 38{com}.         {c )-}
{txt} 39{com}. 
. ********************************************************************************
. * Fill in the rest of the matrix ***********************************************
. ********************************************************************************
. 
. * Get total population analyzed for each group *********************************
. preserve 
{txt} 40{com}. keep if `treat' == 1
{txt} 41{com}. collapse (first) ctownpop_el (first) ctownpop_el2 (first) assigned_ever (first) weight_e1_e2, by(commcode)
{txt} 42{com}. total ctownpop_el [pw = weight_e1_e2] if `treat' == 1
{txt} 43{com}. mat t = e(b)
{txt} 44{com}. global pop_g1 = t[1,1]
{txt} 45{com}. total ctownpop_el2 [pw = weight_e1_e2] if `treat' == 1
{txt} 46{com}. mat t = e(b)
{txt} 47{com}. global pop_g2 = t[1,1]
{txt} 48{com}. restore 
{txt} 49{com}. 
. 
. * Get number of households in the sample, assuming 8 people per household ******
. scalar hh_g1 = $pop_g1 / 8
{txt} 50{com}. scalar hh_g2 = $pop_g2 / 8
{txt} 51{com}. 
. * Input our total decreases for year 1 and year 2 ******************************
. mat net_g1 = ate_g1 * hh_g1
{txt} 52{com}. mat net_g2 = ate_g2 * hh_g2
{txt} 53{com}. 
. 
. * Input the total decline, interpolating the gap between surveys ***************
. mat net_all = 1.5 * net_g1 + 1.5 * net_g2
{txt} 54{com}. 
. 
. 
. 
. ********************************************************************************
. * Merge matrices to form our larger, final matrix. *****************************
. ********************************************************************************
. 
. qui frmttable, statmat(ate_g1) sdec(3) varlabels
{txt} 55{com}. qui frmttable, statmat(ate_g2) sdec(3) varlabels merge
{txt} 56{com}. qui frmttable, statmat(net_g1) sdec(0) varlabels merge
{txt} 57{com}. qui frmttable, statmat(net_g2) sdec(0) varlabels merge
{txt} 58{com}. qui frmttable, statmat(net_all) sdec(0) varlabels merge 
{txt} 59{com}. 
. frmttable using out/tables/`filename', ctitle("", "\uline{c -(}\hfill ATE \hfill{c )-}", "", "\uline{c -(}\hfill Net effect \hfill{c )-}", "", "Net Effect imputed" \ ///
>  "Dependent Variable", "Endline 1", "Endline 2", "Endline 1", "Endline 2", "for missing year") ///
> multicol(1,2,2;1,4,2) ///
> tex ///
> fragment ///
> varlabels ///
> nocenter ///
> replace
{txt} 60{com}. 
. end 
{txt}
{com}. 
. ///

